{
  "name": "api",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "cross-env NODE_ENV=development node ./bin/www",
    "test": "cross-env NODE_ENV=__tests__ jest api.js",
    "script:data": "cross-env NODE_ENV=development node ./script/data",
    "script:secret": "cross-env NODE_ENV=development node ./script/secret",
    "ncu": "run-s ncu:update ncu:clean ncu:install",
