    "cookie-parser": "~1.4.5",
    "cors": "^2.8.5",
    "csurf": "^1.11.0",
    "dotenv": "^10.0.0",
    "ejs": "~3.1.6",
    "express": "~4.17.1",
    "express-validator": "^6.11.1",
    "helmet": "^4.6.0",
    "http-errors": "~1.8.0",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.29.1",
    "moment-timezone": "^0.5.33",
    "nanoid": "^3.1.23",
    "nedb": "^1.8.0",
    "nocache": "^2.1.0",
    "on-finished": "^2.3.0",
    "sequelize": "^6.6.2",
